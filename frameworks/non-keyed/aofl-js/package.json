    "tsd-jsdoc": "2.4.0"
  },
  "dependencies": {
    "@aofl/cli": "3.0.0-beta.42",
    "@aofl/cli-lib": "3.0.0-beta.42",
    "@aofl/component-utils": "3.0.0-beta.42",
    "@aofl/drawer": "3.0.0-beta.42",
