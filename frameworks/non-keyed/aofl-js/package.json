    "@aofl/server-environment": "3.0.0-beta.42",
    "@aofl/store": "3.0.0-beta.42",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "core-js": "3.4.1",
    "isomorphic-fetch": "2.2.1",
